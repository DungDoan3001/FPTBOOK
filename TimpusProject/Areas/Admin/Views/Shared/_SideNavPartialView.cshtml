<!-- Header START -->
            <div class="header">
                <div class="logo logo-dark">
                    <a asp-area="Admin" asp-controller="Home" asp-action="Index">
                        <img src="~/admin_assets/images/logo/logo.png" alt="Logo">
                    </a>
                </div>
                <div class="logo logo-white">
                    <a href="index.html">
                        <img src="~/admin_assets/images/logo/logo-white.png" alt="Logo">
                    </a>
                </div>
                <div class="nav-wrap">
                    <ul class="nav-left">
                        <li class="desktop-toggle">
                            <a href="javascript:void(0);">
                                <i class="anticon"></i>
                            </a>
                        </li>
                        <li class="mobile-toggle">
                            <a href="javascript:void(0);">
                                <i class="anticon"></i>
                            </a>
                        </li>
                    </ul>
                    <ul class="nav-right">
                        <li class="dropdown dropdown-animated scale-left">
                             @foreach(var item in ViewBag.Account)
                                {
                                    <div class="pointer" data-toggle="dropdown">
                                    <div class="avatar avatar-image  m-h-10 m-r-15">
                                            <img src="~/admin_assets/images/avatars/thumb-3.jpg" alt="">
                                        </div>
                                    </div>
                                    <div class="p-b-15 p-t-20 dropdown-menu pop-profile">
                                        <div class="p-h-20 p-b-15 m-b-10 border-bottom">
                                            <div class="d-flex m-r-50">
                                                <div class="avatar avatar-lg avatar-image">
                                                    <img src="@item.Avatar" alt="">
                                                </div>
                                                <div class="m-l-10">
                                                    <p class="m-b-0 text-dark font-weight-semibold">@item.FullName</p>
                                                    <p class="m-b-0 opacity-07">@*@item.Role.RoleName*@</p>
                                                </div>
                                            </div>
                                        </div>
                                        <a href="javascript:void(0);" class="dropdown-item d-block p-h-15 p-v-10">
                                            <div class="d-flex align-items-center justify-content-between">
                                                <div>
                                                    <i class="anticon opacity-04 font-size-16 anticon-user"></i>
                                                    <span class="m-l-10">Edit Profile</span>
                                                </div>
                                                <i class="anticon font-size-10 anticon-right"></i>
                                            </div>
                                        </a>
                                        <a href="/admin-log-out.html" class="dropdown-item d-block p-h-15 p-v-10">
                                            <div class="d-flex align-items-center justify-content-between">
                                                <div>
                                                    <i class="anticon opacity-04 font-size-16 anticon-logout"></i>
                                                    <span class="m-l-10">Logout</span>
                                                </div>
                                                <i class="anticon font-size-10 anticon-right"></i>
                                            </div>
                                        </a>
                                    </div>
                                }
                            
                        </li>
                    </ul>
                </div>
            </div>
            <!-- Header END -->
            <!-- Side Nav START -->
            <div class="side-nav">
                <div class="side-nav-inner">
                    <ul class="side-nav-menu scrollable">
                        <li class="nav-item">
                            <a class="dropdown-toggle" href="javascript:void(0);">
                                <span class="icon-holder">
                                    <i class="fas fa-tachometer-alt"></i>
                                </span>
                                <span class="title">Dashboard</span>
                            </a>
                        </li>
                        <li class="nav-item dropdown open">
                            <a class="dropdown-toggle" href="javascript:void(0);">
                                <span class="icon-holder">
                                    <i class="fas fa-users"></i>
                                </span>
                                <span class="title">Accounts Management</span>
                                <span class="arrow">
                                    <i class="arrow-icon"></i>
                                </span>
                            </a>
                            <ul class="dropdown-menu">
                                <li>
                                    <a asp-area="Admin" asp-controller="Accounts" asp-action="Index">
                                        <span class="icon-holder">
                                            <i class="anticon anticon-arrow-right"></i>
                                        </span>
                                        <span class="title">Admin accounts</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-area="Admin" asp-controller="Customers" asp-action="Index">
                                        <span class="icon-holder">
                                            <i class="anticon anticon-arrow-right"></i>
                                        </span>
                                        <span class="title">Customer accounts</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item dropdown open">
                            <a class="dropdown-toggle" href="javascript:void(0);">
                                <span class="icon-holder">
                                    <i class="fas fa-box-open"></i>
                                </span>
                                <span class="title">Products Management</span>
                                <span class="arrow">
                                    <i class="arrow-icon"></i>
                                </span>
                            </a>
                            <ul class="dropdown-menu">
                                <li>
                                    <a asp-area="Admin" asp-controller="Categories" asp-action="Index">
                                        <span class="icon-holder">
                                            <i class="anticon anticon-arrow-right"></i>
                                        </span>
                                        <span class="title">Categories</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-area="Admin" asp-action="Index" asp-controller="Products">
                                        <span class="icon-holder">
                                            <i class="anticon anticon-arrow-right"></i>
                                        </span>
                                        <span class="title">Products</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-area="Admin" asp-controller="Authors" asp-action="Index">
                                        <span class="icon-holder">
                                            <i class="anticon anticon-arrow-right"></i>
                                        </span>
                                        <span class="title">Author</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-area="Admin" asp-controller="Publishers" asp-action="Index">
                                        <span class="icon-holder">
                                            <i class="anticon anticon-arrow-right"></i>
                                        </span>
                                        <span class="title">Publisher</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item dropdown open">
                            <a class="dropdown-toggle" href="javascript:void(0);">
                                <span class="icon-holder">
                                    <i class="fas fa-file-alt"></i>
                                </span>
                                <span class="title">Orders Management</span>
                                <span class="arrow">
                                    <i class="arrow-icon"></i>
                                </span>
                            </a>
                            <ul class="dropdown-menu">
                                <li>
                                    <a asp-action="Index" asp-area="Admin" asp-controller="Orders">
                                        <span class="icon-holder">
                                            <i class="anticon anticon-arrow-right"></i>
                                        </span>
                                        <span class="title">All</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-action="Pending" asp-area="Admin" asp-controller="Orders">
                                        <span class="icon-holder">
                                            <i class="anticon anticon-arrow-right"></i>
                                        </span>
                                        <span class="title">Pending</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-action="OnGoing" asp-area="Admin" asp-controller="Orders">
                                        <span class="icon-holder">
                                            <i class="anticon anticon-arrow-right"></i>
                                        </span>
                                        <span class="title">On going</span>
                                    </a>
                                </li>
                                <li>
                                    <a asp-action="Finished" asp-area="Admin" asp-controller="Orders">
                                        <span class="icon-holder">
                                            <i class="anticon anticon-arrow-right"></i>
                                        </span>
                                        <span class="title">Finished</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
            <!-- Side Nav END -->